generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid()) @db.Uuid
  email    String @unique
  username String @unique
  password String
  fullname String

  @@map("users")
}

model Authentication {
  token String @unique

  @@map("authentications")
}
